<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <meta http-equiv="Content-Language" content="fr">
        <meta http-equiv="Content-Style-Type" content="text/css">
        <link rel="shortcut icon" href="{{ asset('images/favicon.ico') }}">
        <meta name="robots" content="index, follow">
        <meta name="revisit-after" content="14 days">
        <meta name="Description" content="{% block description '' %}">
        <title>{% block title %}Shinigami Laser Game{% endblock %}</title>
        {% block stylesheets %}
            <!-- Vendor -->
            <link href="{{ asset('front/css/bootstrap.min.css') }}" rel="stylesheet" />
            <link href="{{ asset('front/css/base.css') }}" rel="stylesheet" />
            <link href="{{ asset('front/css/shinigami.front.css') }}" rel="stylesheet" />
            <link href="{{ asset('front/css/font-awesome.min.css') }}" rel="stylesheet" />
            <link href="{{ asset('front/css/shinigami_style.css') }}" rel="stylesheet" />
        {% endblock %}
    </head>

    <body>
        {% block body %}
        <div class="container">
            <div id="logo_banner">
                <div class="row">
                    <div class="col-lg-12">
                        <a href="{{  path('home') }}" class="buttonlogo anim-shine anim-flicker">Shinigami Laser</a>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-12">
                    <div class="page-header">

                      <!-- Navbar
                      ================================================== -->
                     {% include 'front/components/_nav_front.html.twig' %}

                    </div>
                </div>
            </div>
			{% block home_slider '' %}
            <div class="content_main">{% block content '' %}</div>
        </div>

        {% endblock %}

        {% block javascripts %}
        <!-- scripts -->
            <script src="{{ asset('front/js/jquery-3.3.1.min.js') }}" language="JavaScript" type="text/javascript"></script>
            <script src="{{ asset('front/js/bootstrap.min.js') }}" language="JavaScript" type="text/javascript"></script>
        {% endblock %}
        {% block final_javascripts '' %}
    </body>
</html>